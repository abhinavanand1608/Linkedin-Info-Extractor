{
    "manifest_version": 3,
    "name": "Linedin Hunter Extension",
    "version": "1.0",
    "description": "Fetch all the details for a Linedin profile",
    "permissions": ["activeTab", "scripting", "storage"],
    "host_permissions":[
        "https://www.linkedin.com/in/*",
        "https://hunter.io/*"
    ],
    "action": {
        "default_popup": "popup.html"
    }

}